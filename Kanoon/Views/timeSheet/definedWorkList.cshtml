            float: right;
            text-align: center;
            width: 17vw
        }

        .workListItems {
            float: right;
            text-align: center;
            width: 33%;
        }


        .listItems0 {
            float: right;
            text-align: right;
            width: 20vw;
            white-space: nowrap;
            overflow: hidden;
            text-overflow: ellipsis;
        }

        #main-wrapper {
            display: block;
            margin-top: 5vw;
        }


        .form-wrapper {
            width: 100%;
            max-width: 92vw;
        }

        .form-input {
            margin: auto;
            height: 100vw;
            width: 70vw;
            background-color: #daf3c2;
            font-weight: 400;
            font-size: 45px;
            border: none;
            border-radius: 50vw;
            padding: 10vw;
            box-shadow: 0 25px 40px -22px #3c4a56;
        }


        #navbar .menuUserPic {
            width: 20vw;
            height: 20vw;
            border-radius: 50%;
        }

        .userName {
