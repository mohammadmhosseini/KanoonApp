        }

        .dateSelect {
            height: 10vw;
            width: 7.83vw;
            min-width: 7.83vw;
            padding: 0;
            margin-right: 0;
            margin-top: 3vw;
            font-size: 3vw;
            font-weight: 500;
            border: none;
            border-radius: 2vw;
            background-color: antiquewhite;
            box-shadow: 0 25px 40px -22px #3c4a56;
        }

        .dateTitle {
            height: 10vw;
            width: 20vw;
            min-width: 200px;
            padding: 2vw 0;
            margin-right: 2.5vw;
            margin-top: 3vw;
            font-size: 3vw;
            font-weight: 600;
            border: none;
        }

        .ActiveItem {
            color: limegreen;
            width: 100%;
            background-color: white;
            border-radius: 1vw;
            padding: 1vw 2vw 2vw 2vw;
            margin: 1vw;
            line-height: 1.5;
            align-items: center;
            font-size: 2vw;
            min-height: 6vw;
        }

        .DeactiveItem {
            color: orangered;
            width: 100%;
            background-color: white;
            border-radius: 1vw;
            padding: 1vw 2vw 2vw 2vw;
            margin: 1vw;
            line-height: 1.5;
            align-items: center;
            font-size: 2vw;
            min-height: 6vw;
        }

        .UItem {
            width: 100%;
            background-color: white;
            border-radius: 1vw;
            padding: 1vw 2vw 2vw 2vw;
            margin: 1vw;
