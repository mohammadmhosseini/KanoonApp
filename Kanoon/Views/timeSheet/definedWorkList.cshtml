    <div id="main-wrapper">
        <div class="form-pop" id="EditWorkTitlePopUp" style="display: none;">
            <div class="form-cont" id="form-cont">
                <h1 class="popLable1" id="setEditWorkLabel"></h1>
                <input type="text" id="EditWorkTitle" class="description" placeholder="توضیحات" />
                <button type="submit" class="btn" onclick="EditWorkTitle()" id="formButton4" style="margin: 2% 0%;">ثبت</button>
                <button class="btn cancel" onclick="closeEditWorkTitle()" id="formButtonC4" style="margin: 2% 0%;">بستن</button>
            </div>
        </div>

        <div class="form-pop" id="insertNewWorkPopUp" style="display: none;">
            <div class="form-cont" id="form-cont">
                <h1 class="popLable1" id="setNewWorkLabel"></h1>
                <input type="text" id="newWorkTitle" class="description" placeholder="توضیحات" />
                <button type="submit" class="btn" onclick="insertNewDefinedWork()" id="formButton4" style="margin: 2% 0%;">ثبت</button>
                <button class="btn cancel" onclick="closeNewWorkForm()" id="formButtonC4" style="margin: 2% 0%;">بستن</button>
            </div>
        </div>

        <div class="form-wrapper" id="users">
            <ul id="userList1" class="CreditList" style="width:18vw; display:none">
